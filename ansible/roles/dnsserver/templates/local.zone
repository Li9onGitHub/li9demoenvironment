$ORIGIN .
$TTL 1 ; 1 seconds (for testing only)
{{localdomain}} IN SOA master.{{localdomain}}. root.{{localdomain}}. (
	2011112904 ; serial
	60 ; refresh (1 minute)
	15 ; retry (15 seconds)
	1800 ; expire (30 minutes)
	10 ; minimum (10 seconds)
)
	IN NS master01.{{localdomain}}.
$ORIGIN {{localdomain}}.
* IN  A {{ansible_eth0.ipv4.address}}
master01 IN A {{ansible_eth0.ipv4.address}}
node01   IN A 10.0.0.12
node02   IN A 10.0.0.13
